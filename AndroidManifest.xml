<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.doubibi.superclubmanager"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity android:name=".Aty_Welcome" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Aty_Main"
            android:label="@string/app_name" >
        </activity>
        <activity android:name=".Aty_Guide" ></activity>
        <activity android:name=".Aty_ActivityList"
            android:parentActivityName=".Aty_Main"></activity>
        <activity android:name="Aty_ActivityEdit"></activity>
        <activity android:name=".Aty_PeopleArrange"></activity>
        <activity android:name=".Aty_PeopleList"></activity>
    </application>

</manifest>